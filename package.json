{
  "name": "teamwork-app",
  "version": "1.0.0",
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "description": "social network app",
  "main": "server.js",
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "heroku-prebuild": "rm -rf node_modules",
    "heroku-postbuild": "npm install",
    "start": "babel-node ./src/server.js",
    "start:dev": "nodemon --exec babel-node ./src/server.js",
    "lint": "eslint ./src --fix",
    "test": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MicahBala/Teamwork-app.git"
  },
  "author": "Micah Bala",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MicahBala/Teamwork-app/issues"
  },
  "homepage": "https://github.com/MicahBala/Teamwork-app#readme",
  "dependencies": {
    "babel-plugin-dotenv": "^0.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.17.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.9",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.8",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "nyc": "^14.1.1",
    "regenerator-runtime": "^0.13.3",
    "supertest": "^4.0.2"
  }
}