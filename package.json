{
  "name": "teamwork-app",
  "version": "1.0.0",
  "description": "social network app",
  "main": "server.js",
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/server.js",
    "start:dev": "nodemon --exec babel-node ./src/server",
    "lint": "eslint ./src --fix",
    "test": "jest --watchAll --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MicahBala/Teamwork-app.git"
  },
  "author": "Micah Bala",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MicahBala/Teamwork-app/issues"
  },
  "homepage": "https://github.com/MicahBala/Teamwork-app#readme",
  "dependencies": {
    "babel-plugin-dotenv": "^0.1.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.17.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.6",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "regenerator-runtime": "^0.13.3",
    "supertest": "^4.0.2"
  }
}
